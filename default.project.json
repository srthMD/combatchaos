{
  "name": "combatchaos",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Data": {
        "$path": "src/data"
      },

      "Packages": {
        "$path": "Packages"
      },

      "Shared": {
        "$path": "src/clientshared"
      },

      "Client": {
        "$path": "src/client"
      }
    },

    "ServerStorage": {
      "Shared": {
        "$path": "src/servershared"
      },

      "Server": {
        "$path": "src/server"
      }
    },

    "ServerScriptService": {
      "Loaders": {
        "$path": "src/loaders/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Loaders": {
          "$path": "src/loaders/client"
        }
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
